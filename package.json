{
  "name": "@eclass/eslint-config",
  "version": "3.0.1",
  "description": "eslint shareable config",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write '*.{js,json,md}' '.prettierrc' '.{github,vscode}/**/*'",
    "format:check": "prettier --check '*.{js,json,md}' '.prettierrc' '.{github,vscode}/**/*'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclass/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eclass"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eclass/eslint-config/issues"
  },
  "homepage": "https://github.com/eclass/eslint-config#readme",
  "dependencies": {
    "eslint-config-standard": "^17.0.0-1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.1",
    "eslint-plugin-n": "14.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^37.0.0 || ^38.0.0 || ^39.0.0",
    "eslint-plugin-n": "^14.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "<1.0.0"
  }
}
